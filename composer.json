{
    "name": "soapbox/signed-requests",
    "description": "A wrapper to add the ability to accept signed requests to a Laravel project.",
    "keywords": ["laravel", "signed", "requests", "signed-requests"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Soapbox/fake-requests"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0",
        "nesbot/carbon": "^2.0",
        "ramsey/uuid": "^3.6"
    },
    "require-dev": {
        "jshayes/fake-requests": "dev-master",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\SignedRequests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
